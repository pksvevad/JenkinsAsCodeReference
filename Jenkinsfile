pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        echo 'build backend'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'test backend'
      }
    }
    stage('More Tests') {
      steps {
        echo 'more tests'
      }
    }
    stage('Deploy Staging') {
      steps {
        echo 'deploy staging'
      }
    }
    stage('Deploy Production') {
      steps {
        echo 'deploy production'
      }
    }
  }
}